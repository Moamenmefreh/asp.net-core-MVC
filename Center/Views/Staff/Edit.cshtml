@model Student
@{
    ViewData["Title"] = "Edit";
}
@if (TempData["Success"] != null)
{
    <h2>@TempData["Success"]</h2>
}
<form method="post" class="form-control w-75">


   

    <label asp-for="Name" class="form-control btn btn-primary m-2"></label>
    <input asp-for="Name" class="form-control m-2" />
    <span asp-validation-for="Name"></span>


    <label asp-for="Level" class="form-control btn btn-primary m-2"></label>
    <input asp-for="Level" class="form-control m-2" />
    <span asp-validation-for="Level"></span>
    <input type="submit" class="btn btn-primary  w-50 m-5" value="Edit" />
    <a asp-action="AllStudent" asp-controller="Staff" class="btn btn-info m-3">
        Back To All Student
    </a>



</form>
